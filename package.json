{
  "name": "stlviewer",
  "version": "0.0.1",
  "description": "Javascript STL/OBJ 3D files Viewer",
  "source": "src/StlViewer.js",
  "targets": {
    "default": {
        "publicUrl": ".",
        "includeNodeModules": ["three"]
    },
    "node": {
        "publicUrl": ".",
        "engines": {
            "node": ">= 8.0.0"
        }
    },
    "modern": {
        "publicUrl": ".",
        "engines": {
          "browsers": "Chrome 80"
        }
      },
    "legacy": {
        "publicUrl": ".",
        "engines": {
          "browsers": "> 0.5%, last 2 versions, not dead"
        }
      }
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "watch": "parcel watch",
    "build": "npm run clean && parcel build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SimplyPrint/StlViewer.git"
  },
  "author": {
    "name": "SimplyPrint",
    "email": "contact@simplyprint.io",
    "url": "https://simplyprint.io"
  },
  "contributors": [
    "omrips",
    "simplyprint"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SimplyPrint/StlViewer/issues"
  },
  "homepage": "https://github.com/SimplyPrint/StlViewer#readme",
  "publishConfig": {
    "@cjavad:registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@parcel/babel-preset-env": "^2.7.0",
    "parcel": "latest"
  },
  "dependencies": {
    "three": "^0.140.2"
  }
}
